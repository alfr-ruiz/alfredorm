{
  "hosting": {
    "public": "src",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "cleanUrls": true,
    "rewrites": [
      {
        "source": "/",
        "destination": "/index.html"
      },
      {
        "source": "/me",
        "destination": "/me.html"
      },
      {
        "source": "/contact",
        "destination": "/contact.html"
      }
    ]
  }
}
